<comment>
problem   = underdense Mach10 jet
reference = 
configure = --prob=jet 
note      = gamma=1.1 | domain=150×30 | resolution=1000×300 | tlim=150 | B-field=on
<job>
problem_id = jet-cool-b      # problem ID: basename of output filenames


<output1>
file_type  = hst      # History data dump
dt         = 0.1      # time increment between outputs

<output2>
file_type  = vtk      # Binary data dump
variable   = prim     # variables to be output
dt         = 1.0      # time increment between outputs

<time>
cfl_number = 0.4      # The Courant, Friedrichs, & Lewy (CFL) Number
nlim       = -1       # cycle limit
tlim       = 150.0    # time limit
integrator  = vl2       # time integration algorithm
xorder      = 2         # order of spatial reconstruction
ncycle_out  = 1         # interval for stdout summary info

<mesh>
nx1        = 1000      # Number of zones in X1-direction
x1min      = 0.0      # minimum value of X1
x1max      = 100.0     # maximum value of X1
ix1_bc     = user     # inner-X1 boundary flag
ox1_bc     = outflow  # outer-X1 boundary flag

nx2        = 300      # Number of zones in X2-direction
x2min      = -15.0    # minimum value of X2
x2max      = 15.0     # maximum value of X2
ix2_bc     = outflow  # inner-X2 boundary flag
ox2_bc     = outflow  # outer-X2 boundary flag

nx3        = 1        # Number of zones in X3-direction
x3min      = -15.0    # minimum value of X3
x3max      = 15.0     # maximum value of X3
ix3_bc     = outflow  # inner-X3 boundary flag
ox3_bc     = outflow  # outer-X3 boundary flag

<hydro>
gamma   = 1.1     # gamma = C_p/C_v

<problem>
d  = 1.0
p  = 6.0e-3
vx = 0.0
vy = 0.0
vz = 0.0
bx = 0.1
by = 0.0
bz = 0.0

djet = 0.1
pjet = 6.0e-3
vxjet = 1.0
vyjet = 0.0
vzjet = 0.0 

# no magnetic field
bxjet = 0.1
byjet = 0.0
bzjet = 0.0

rjet = 1.0
